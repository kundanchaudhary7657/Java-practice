class Main {
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        int arr[]={1,2,4,7,7,5};
        int largest=arr[0];
        int sl=-1;
        for(int i=1;i<arr.length;i++ ){
            if(arr[i]>largest){
                sl=largest;
                largest=arr[i];
            }
            else if(arr[i]<largest && arr[i]>sl){
                sl=arr[i];
            }
        }
        System.out.println(sl);
    }
}
