class Solution {
    public int majorityElement(int[] nums) {
        Map<Integer,Integer>mpp= new HashMap<>();
        
        for(int i=0;i<nums.length;i++){
            int num= nums[i];
            mpp.put(num,mpp.getOrDefault(num,0)+1);

            if(mpp.get(num)>nums.length/2){
            return num;
        }
        }
        
        return -1;
    }
}
