import java.util.Arrays;

class Main {
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        int arr[] = {1, 2, 3, 4, 5};

        Arrays.sort(arr);

        int max = arr[arr.length - 1]; 
        System.out.println("Maximum value is: " + max);
    }
}
